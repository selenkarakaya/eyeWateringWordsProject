  "author": "Selen Karakaya",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "mongoose": "^8.5.1",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "papercss": "^1.9.2",
    "primereact": "^10.8.3",
    "react-avatar-edit": "^1.2.0",
    "react-image-crop": "^11.0.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "testEnvironment": "jsdom"
